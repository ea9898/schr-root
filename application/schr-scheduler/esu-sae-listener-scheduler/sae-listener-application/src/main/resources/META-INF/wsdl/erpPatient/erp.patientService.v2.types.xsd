<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://emias.mos.ru/erp/patientService/v2/types/"
           xmlns:nsf="http://emias.mos.ru/system/v1/faults/"
           xmlns:core="http://emias.mos.ru/patientService/core/v1/"
           xmlns:adr="http://emias.mos.ru/patientService/address/core/v1/"
           targetNamespace="http://emias.mos.ru/erp/patientService/v2/types/"
           xmlns="http://emias.mos.ru/erp/patientService/v2/types/"
           version="1.0.0">

    <xs:include schemaLocation="../emias/emias.core.v1.xsd"/>
    <xs:include schemaLocation="../emias/emias.core.address.v1.xsd"/>
    <xs:import namespace="http://emias.mos.ru/system/v1/faults/" schemaLocation="../emias/emias.system.v1.faults.xsd"/>
    <xs:import namespace="http://emias.mos.ru/patientService/core/v1/" schemaLocation="erp.patientService.core.v1.xsd"/>
    <xs:import namespace="http://emias.mos.ru/patientService/address/core/v1/" schemaLocation="erp.patientService.address.core.v1.xsd"/>

    <!--БЛОК: Базовые комплексные типы-->

    <xs:complexType name="Snils">
        <xs:sequence>
            <xs:element name="typeSnils" type="core:TypeSnils" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Источник СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="numSnils" type="core:NumSnils" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateChangeSnils" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата последнего обновления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="snilsValidStatus" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус валидации СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="snilsValidDate" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата валидации СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="snilsAbsenceReason" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Причина отсутствия СНИЛС в ЕМИАС</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SnilsBanner">
        <xs:complexContent>
            <xs:extension base="tns:Snils">
                <xs:sequence>
                    <xs:element name="SnilsAbsenceReason" type="tns:CodeWithName" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Причина отсутствия СНИЛС в ЕМИАС</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>    
    </xs:complexType> 

    <xs:complexType name="SnilsInfo">
        <xs:sequence>
            <xs:element name="snilsAbsenceReasonGlobalId" type="ID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор причины отсутствия СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BirthPlaceInfo">
        <xs:sequence>
            <xs:element name="birthPlace" type="core:BirthPlace" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DocumentDivisionInfo">
        <xs:sequence>
            <xs:element name="documentDivisionCode" type="core:DocumentDivisionCode" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="EducationStateInfo">
        <xs:sequence>
            <xs:element name="educationStateGlobalId" type="ID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор образования</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
        
    <xs:complexType name="StudyInfo">
        <xs:sequence>              
            <xs:element name="class" type="core:ClassOrGroup" minOccurs="0"/>                 
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="StudyInfoWithGlobalId">
        <xs:complexContent>
            <xs:extension base="tns:StudyInfo">
                <xs:sequence>
                    <xs:element name="educationFacilityGlobalId" type="ID" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Идентификатор образовательной организации</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>    
    </xs:complexType> 
    
    <xs:complexType name="StudyInfoWithoutGlobalId">
        <xs:complexContent>
            <xs:extension base="tns:StudyInfo">
                <xs:sequence>
                    <xs:element name="educationFacility" type="tns:CodeWithName" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Образовательная организация</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="educationFacilityAddress" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Адрес образовательной организации</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>    
    </xs:complexType> 

    <xs:complexType name="SocialStatusEmploymentEdit">
        <xs:sequence>
            <xs:element name="socialStatusGlobalId" type="ID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор социального статуса, в том числе занятости</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="armyCode" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код военнослужащего</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="isDependent" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Иждивенец (если «Да»)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="educationStateInfo" type="tns:EducationStateInfo" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения об образовании пациента</xs:documentation>
                </xs:annotation>
            </xs:element>            
            <xs:element name="works" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="workInfo" type="tns:WorkInfoEdit" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Сведения о местах работы/учебы</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PatientSearch">
        <xs:sequence>
            <xs:element name="region" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="regionCode" type="core:PatientRegionCode" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Категория пациента</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="isAlive" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Признак - искать живых</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="disability" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="disabilityCode" type="xs:string" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Код группы инвалидности</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="moOfAttach" type="tns:ID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор МО прикрепления (идентификатор головной МО прикрепления)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="muOfAttach" type="tns:ID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор МУ прикрепления (идентификатор филиала)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="areaId" type="tns:ID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор участка прикрепления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyNumber" type="core:PolicyNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер полиса ОМС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="snils" type="core:NumSnils" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="document" type="tns:DocumentSearch" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ, удостоверяющий личность пациента</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="fullName" type="core:FullName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>ФИО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="birthDate" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="startDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата начала периода</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="endDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>Дата окончания периода</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="genderCode" type="core:GenderCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Пол пациента</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="phoneNumber" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер телефона</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="email" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Адрес электронной почты</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SocialStatusEmployment">
        <xs:sequence>
            <xs:element name="socialStatus" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Социальный статус</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="armyCode" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код военнослужащего</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="isDependent" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Иждивенец (если «Да»)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="works" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="workInfo" type="tns:WorkInfo" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Сведения о месте работы/учебы</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SocialStatusEmploymentBanner">
        <xs:sequence>
            <xs:element name="socialStatus" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Социальный статус</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="armyCode" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Код военнослужащего</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="isDependent" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Иждивенец (если «Да»)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="educationState" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Образование пациента</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="works" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="workInfo" type="tns:WorkInfoBanner" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Сведения о месте работы/учебы</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>           
    </xs:complexType> 

    <xs:complexType name="WorkInfo">
        <xs:sequence>
            <xs:element name="workId" type="tns:ID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Идентификатор места работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="isWorkStudy" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Признак места работы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workPlace" type="core:WorkValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Место работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workProfession" type="core:WorkValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Профессия</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workPosition" type="core:WorkValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Должность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workBeginDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата начала работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workEndDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workStatus" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус места работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workChangeDate" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата последнего изменения места работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="WorkInfoBanner">
        <xs:complexContent>
            <xs:extension base="tns:WorkInfo">
                <xs:sequence>
                    <xs:element name="studyInfo" type="tns:StudyInfoWithoutGlobalId" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Сведения об образовательной организации места учебы пациента</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>    
    </xs:complexType> 

    <xs:complexType name="WorkInfoEdit">
        <xs:sequence>
            <xs:element name="isWorkStudyCode" type="core:IsWorkStudyCode">
                <xs:annotation>
                    <xs:documentation>Признак места работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workPlace" type="core:WorkValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Место работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workProfession" type="core:WorkValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Профессия</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workPosition" type="core:WorkValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Должность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workBeginDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата начала работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="workEndDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="studyInfo" type="tns:StudyInfoWithGlobalId" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения об образовательной организации места учебы пациента</xs:documentation>
                </xs:annotation>
            </xs:element>      
            <xs:element name="workStatusCode" type="core:StatusCodeWithRemove" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус записи о месте работы/учебы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PolicyEdit">
        <xs:sequence>
            <xs:element name="policyOMSTypeCode" type="core:PolicyOMSTypeCode">
                <xs:annotation>
                    <xs:documentation>Вид полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyNumber" type="core:PolicyNumber">
                <xs:annotation>
                    <xs:documentation>Номер полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyBeginDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата начала действия полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyEndDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policySMOGlobalId" type="ID">
                <xs:annotation>
                    <xs:documentation>Код страховой компании</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policySMOBeginDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата начала прикрепления к СМО</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyStatusCode" type="core:PolicyStatusCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Policy">
        <xs:sequence>
            <xs:element name="policyOMSType" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Вид полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyNumber" type="core:PolicyNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyBeginDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата начала действия полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyEndDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания действия полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyInsurance" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="policySMOGlobalId" type="xs:long" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Идентификатор страховой медицинской организации в СУ НСИ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="isExternal" type="xs:boolean" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Признак инотерриториальной СМО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policyInsuranceTerritoryCode" type="core:PolicyInsuranceTerritoryCode" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Код территории страхования</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policyInsuranceTerritoryName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Наименование региона территории страхования</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policySMOOGRN" type="core:PolicySMOOGRN" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>ОГРН страховой медицинской организации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policySMOCode" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Код страховой медицинской организации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policySMOName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Наименование страховой медицинской организации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policySMOShortName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Краткое наименование страховой медицинской организации</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policySMOBeginDate" type="xs:date" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дата начала прикрепления к СМО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policySMOEndDate" type="xs:date" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Дата окончания прикрепления к СМО</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="policyStatus" type="tns:CodeWithName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус полиса</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="policyOfErzl" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="policyErzlStatus" type="xs:string" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>Статус полиса в РС ЕРЗЛ</xs:documentation>
