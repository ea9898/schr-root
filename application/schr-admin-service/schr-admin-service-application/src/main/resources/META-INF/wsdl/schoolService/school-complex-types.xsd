<?xml version="1.0"?>
<xs:schema version="1.0"
           elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="1.0"
           xmlns:tns="http://erp.ptnl.net/school/v1/school-service/model"
           targetNamespace="http://erp.ptnl.net/school/v1/school-service/model">

    <xs:include schemaLocation="common-types.xsd" />
    <xs:include schemaLocation="school-simple-types.xsd" />
    
    <xs:complexType name="EmiasIds">
	    <xs:sequence>
	        <xs:element name="emiasId" type="tns:EmiasId" maxOccurs="unbounded">
	            <xs:annotation>
	                <xs:documentation>
	                    Идентификатор пациента в ЕМИАС
	                </xs:documentation>
	            </xs:annotation>
	        </xs:element>
	    </xs:sequence>
	</xs:complexType>
    
    <!-- Requests - Responses -->
    <xs:complexType name="GetRepresentativesRequest">
        <xs:annotation>
            <xs:documentation>
                Запрос на получение данных представителей идентифицированных учащихся
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="patients">
                <xs:annotation>
                    <xs:documentation>
                        Группирующая сущность - "Пациенты"
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="emiasId" type="tns:EmiasId" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>
                                    Идентификатор участка (школьного класса)
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GetRepresentativesResponse">
        <xs:annotation>
            <xs:documentation>
                Ответ на запрос на получение данных представителей идентифицированных учащихся
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="resultRequests">
                <xs:annotation>
                    <xs:documentation>
                        Группирующая сущность результатов выполнения запроса
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="resultRequest" minOccurs="1" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>
                                    Результат выполнения запроса
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="emiasId" type="tns:EmiasId">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Идентификатор пациента в ЕМИАС
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="result" type="tns:Result">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Результат выполнения запроса. Возможные значения: success – успешно; failure – не успешно
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="studentRepresentatives" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Группирующая сущность, которая содержит данные о представителях учащегося - пациента
                                            </xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="comment" type="tns:Comment" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Комментарий в формате текста
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="representatives" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Группирующая сущность - "Представители"
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="representative" maxOccurs="unbounded">
                                                                <xs:annotation>
                                                                    <xs:documentation>
                                                                        Группирующая сущность - "Представитель"
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                                <xs:complexType>
                                                                    <xs:sequence>
                                                                        <xs:element name="representativeId" type="tns:RepresentativeId">
                                                                            <xs:annotation>
                                                                                <xs:documentation>
                                                                                    Идентификатор представителя (соответствует значению person_uuid в КИС ГУСОЭВ)
                                                                                </xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:element>
                                                                        <xs:element name="family" type="tns:RepFamily">
                                                                            <xs:annotation>
                                                                                <xs:documentation>
                                                                                    Фамилия представителя
                                                                                </xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:element>
                                                                        <xs:element name="name" type="tns:RepName">
                                                                            <xs:annotation>
                                                                                <xs:documentation>
                                                                                    Имя представителя
                                                                                </xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:element>
                                                                        <xs:element name="patronymic" type="tns:RepPatronymic" minOccurs="0">
                                                                            <xs:annotation>
                                                                                <xs:documentation>
                                                                                    Отчество представителя
                                                                                </xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:element>
                                                                        <xs:element name="contactRepresentative" type="tns:RepContact"
                                                                                    minOccurs="0" maxOccurs="unbounded">
                                                                            <xs:annotation>
                                                                                <xs:documentation>
                                                                                    Группирующая сущность – "Контакты представителя"
                                                                                </xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:element>
                                                                    </xs:sequence>
                                                                </xs:complexType>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="errors" type="tns:Errors" minOccurs="0">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Сообщения об ошибке
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GetPatientBannerRequest">
        <xs:annotation>
            <xs:documentation>
                Запрос на получение баннера учащегося
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="emiasId" type="tns:EmiasId">
                <xs:annotation>
                    <xs:documentation>
                        Идентификатор пациента в ЕМИАС
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GetPatientBannerResponse">
        <xs:annotation>
            <xs:documentation>
                Ответ на запрос на получение баннера учащегося
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="student">
                <xs:annotation>
                    <xs:documentation>
                        Группирующая сущность - "Сведения об учащемся"
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="studentId" type="tns:StudentId">
                            <xs:annotation>
                                <xs:documentation>
                                    Идентификатор учащегося (соответствует значению person_uuid в КИС ГУСОЭВ)
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ukl" type="tns:Ukl">
                            <xs:annotation>
                                <xs:documentation>
                                    УКЛ пациента
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="emiasId" type="tns:EmiasId">
                            <xs:annotation>
                                <xs:documentation>
                                    Идентификатор пациента в ЕМИАС
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="family" type="tns:PatFamily">
                            <xs:annotation>
                                <xs:documentation>
                                    Фамилия
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="name" type="tns:PatName">
                            <xs:annotation>
                                <xs:documentation>
                                    Имя
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="patronymic" type="tns:PatPatronymic" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Отчество
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="gender" type="tns:Gender">
                            <xs:annotation>
                                <xs:documentation>
                                    Пол учащегося. Возможные значения: 1 – мужской; 2 – женский; 0 – любой
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="birthDate" type="xs:date">
                            <xs:annotation>
                                <xs:documentation>
                                    Дата рождения
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="policies" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Группирующая сущность - "Сведения об полисах пациента"
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="policy" minOccurs="1" maxOccurs="unbounded" type="tns:OutputPolicy">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Сведения об актуальном полисе пациента.
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="documents" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Группирующая сущность - "Сведения об актуальных документах пациента"
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="document" minOccurs="1" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Группирующая сущность. Только актуальные документы
                                            </xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="documentTypeId" type="tns:DocumentTypeId">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Код типа документа
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="documentTypeName" type="tns:DocumentTypeName">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Наименование типа документа. Заполняется на основании справочника ЕМИАС "Тип ДУЛ". Справочник синхронизируется с ПО Норма (DOC_TYPE)
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="documentSeries" type="tns:DocumentSeries" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Серия документа
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="documentNumber" type="tns:DocumentNumber">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Номер документа
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="snils" type="tns:Snils" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    СНИЛС
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="age" type="tns:Age" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Количество полных лет
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="contactsPatient" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Группирующая сущность - "Контакты пациента" (только актуальные)
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="contactPatient" type="tns:PatContact" minOccurs="1" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Группирующая сущность - "Контакт пациента"
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="addresses" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Группирующая сущность - "Адреса пациента" (только актуальные)
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="address" type="tns:OutputAddress" minOccurs="1" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Группирующая сущность - "Адрес пациента"
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="attachments" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Группирующая сущность. Возвращаются актуальные прикрепления пациента к МО (прикрепление к ОО исключается)
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="attachment" minOccurs="1" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Группирующая сущность - "Прикрепление"
                                            </xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="attachId" type="tns:AttachId">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Идентификатор прикрепления, созданного в ЕМИАС.ЕРП
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="attachNumber" type="tns:AttachNumber" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Номер прикрепления ЕМИАС
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="attachType" type="tns:StudAttachType">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Категория прикрепления. Допустимые значения: 0 – небазовое; 1 – базовое терапевтическое; 2 – базовое стоматологическое; 9 – прикрепление к ОО
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="attachFormID" type="tns:AttachFormID">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Код вида прикрепления. Заполняется в соответствии со справочником ЕМИАС "Вид прикрепления". Возможные значения: 10 – по участковому принципу (основное); 20 – по участковому принципу; 40 – по медицинским показаниям
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="attachFormName" type="tns:AttachFormName">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Наименование вида прикрепления. Заполняется на основании справочника ЕМИАС "Вид прикрепления". Справочник синхронизируется с ПО Норма (ATTACHMENT_TYPE)
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="mainMoId" type="tns:MainMoId" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Идентификатор головной МО прикрепления в ЕМИАС
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="moId" type="tns:MoId">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Идентификатор МО (филиала) прикрепления в ЕМИАС
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="areaId" type="tns:AreaId" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Идентификатор участка прикрепления
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="areaTypeId" type="tns:AreaTypeId" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Тип участка прикрепления. Заполняется в соответствии со справочником типов участков в ЕМИАС. Только цифры
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="areaTypeName" type="tns:AreaTypeName" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Наименование типа участка прикрепления. Заполняется на основании справочника типов участков в ЕМИАС. Справочник синхронизируется с ПО Норма (AREA_TYPE)
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="dateFromAttach" type="xs:date">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Дата начала прикрепления. Формат: "YYYY-MM-DD"
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="representatives" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Группирующая сущность - "Представители Учащегося - пациента"
                                </xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="representative" minOccurs="1" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>
                                                Группирующая сущность - "Представитель Учащегося - пациента"
                                            </xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="representativeId" type="tns:RepresentativeId">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Идентификатор представителя (соответствует значению person_uuid в КИС ГУСОЭВ)
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="family" type="tns:RepFamily">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Фамилия
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="name" type="tns:RepName">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Имя
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="patronymic" type="tns:RepPatronymic" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Отчество
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="contactsRepresentative" minOccurs="0">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Группирующая сущность – "Контакты представителя"
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element name="contactRepresentative" type="tns:RepContact" minOccurs="1" maxOccurs="unbounded">
                                                                <xs:annotation>
                                                                    <xs:documentation>
                                                                        Группирующая сущность – "Контакт представителя"
                                                                    </xs:documentation>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="comment" type="tns:Comment" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Комментарий
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GetStudentsListRequest">
        <xs:annotation>
            <xs:documentation>
                Запрос на поиск пациента по параметрам
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="policyNumber" type="tns:PolicyNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Номер полиса ОМС
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="document" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>
                        Группирующая сущность - "Документ, удостоверяющий личность"
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="documentTypeId" type="tns:DocumentTypeId">
                            <xs:annotation>
                                <xs:documentation>
                                    Код типа документа. Заполняется на основании справочника ЕМИАС "Тип ДУЛ". Справочник синхронизируется с ПО Норма (DOC_TYPE)
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="documentSeries" type="tns:DocumentSeries" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    Серия ДУЛ
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="documentNumber" type="tns:DocumentNumber">
                            <xs:annotation>
